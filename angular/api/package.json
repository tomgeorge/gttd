{
  "name": "api",
  "version": "1.0.0",
  "description": "Express API server to hit mongo and redis",
  "main": "index.js",
  "scripts": {
    "build": "gulp",
    "server": "webpack-dev-server --inline --progress --colors --display-modules --history-api-fallback --host 0.0.0.0",
    "dev": "webpack-dev-server --inline --progress --colors --display-modules --history-api-fallback --host 0.0.0.0",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "node dist/src/main/api/index.js",
    "postinstall": "typings install",
    "typings": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomgeorge/gttd.git"
  },
  "author": "Tom George",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomgeorge/gttd/issues"
  },
  "homepage": "https://github.com/tomgeorge/gttd#readme",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "errorhandler": "^1.4.2",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "express-unless": "^0.3.0",
    "jsonwebtoken": "^7.0.1",
    "lodash": "^4.13.1",
    "mongoose": "^4.5.0"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-inject": "^4.1.0",
    "gulp-rimraf": "^0.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.6",
    "superstatic": "^4.0.2",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  }
}
